{
  "name": "andersundsehr/sentry-async",
  "description": "Asynchronous Sentry for Symfony - Fire and forget",
  "license": "MIT",
  "type": "symfony-bundle",
  "keywords": [
    "logging",
    "errors",
    "symfony",
    "sentry"
  ],
  "authors": [
    {
      "name": "Stefan Lamm",
      "email": "s.lamm@andersundsehr.com"
    }
  ],
  "homepage": "https://www.andersundsehr.com",
  "require": {
    "php": "~8.2.0 || ~8.3.0",
    "ext-fileinfo": "*",
    "guzzlehttp/guzzle": "*",
    "php-http/discovery": "*",
    "psr/http-client": "*",
    "psr/http-factory": "*",
    "psr/http-message": "*",
    "sentry/sentry": "^4.10.0"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "^7.1.0",
    "symfony/http-kernel": "^6.4 || ^7.0",
    "symfony/test-pack": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "AUS\\SentryAsync\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "php-http/discovery": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true
    },
    "sort-packages": true
  }
}
